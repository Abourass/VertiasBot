  "dependencies": {
    "@octokit/app": "^2.2.2",
    "@octokit/request": "^2.4.2",
    "@octokit/rest": "^16.20.0",
    "connect-mongo": "^2.0.3",
    "cookie-parser": "^1.4.4",
    "express-session": "^1.15.6",
    "mongoose": "^5.4.19",
    "passport": "^0.4.0",
    "probot": "^9.0.2",
    "request": "^2.88.0"
  },
  "devDependencies": {
    "jest": "^24.0.0",
    "nock": "^10.0.0",
    "nodemon": "^1.17.2",
    "smee-client": "^1.0.2",
    "standard": "^12.0.1"
  },
  "engines": {
    "node": ">= 8.3.0"
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "nodemonConfig": {
    "exec": "npm start",
    "watch": [
      ".env",
      "."
    ]
  },
  "jest": {
    "testEnvironment": "node"
  }
